{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "cart": {
      "projectType": "application",
      "schematics": {
        "@schematics/angular:component": {
          "style": "scss"
        }
      },
      "root": "",
      "sourceRoot": "src",
      "prefix": "app",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "allowedCommonJsDependencies": [
              "firebase",
              "@firebase/app",
              "firebase/app",
              "@firebase/auth",
              "notiflix",
              "lodash"
            ],
            "outputPath": "dist/cart",
            "index": "src/index.html",
            "main": "src/main.ts",
            "polyfills": "src/polyfills.ts",
            "tsConfig": "tsconfig.app.json",
            "aot": true,
            "assets": [
              "src/favicon.ico",
              "src/assets",
              "src/manifest.webmanifest"
            ],
            "styles": [
              "src/styles.scss"
            ],
            "scripts": []
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "src/environments/environment.ts",
                  "with": "src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "6kb",
                  "maximumError": "10kb"
                }
              ],
              "serviceWorker": true,
              "ngswConfigPath": "ngsw-config.json"
            }
          }
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "cart:build"
          },
          "configurations": {
            "production": {
              "browserTarget": "cart:build:production"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "cart:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "src/test.ts",
            "polyfills": "src/polyfills.ts",
            "tsConfig": "tsconfig.spec.json",
            "karmaConfig": "karma.conf.js",
            "assets": [
              "src/favicon.ico",
              "src/assets",
              "src/manifest.webmanifest"
            ],
            "styles": [
              "src/styles.scss"
            ],
            "scripts": []
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "tsconfig.app.json",
              "tsconfig.spec.json",
              "e2e/tsconfig.json",
              "tsconfig.server.json"
            ],
            "exclude": [
              "**/node_modules/**"
            ]
          }
        },
        "e2e": {
          "builder": "@angular-devkit/build-angular:protractor",
          "options": {
            "protractorConfig": "e2e/protractor.conf.js",
            "devServerTarget": "cart:serve"
          },
          "configurations": {
            "production": {
              "devServerTarget": "cart:serve:production"
            }
          }
        }
      }
    },
    "shop-version-2": {
      "projectType": "application",
      "schematics": {
        "@schematics/angular:component": {
          "style": "scss"
        }
      },
      "root": "projects/shop-version-2",
      "sourceRoot": "projects/shop-version-2/src",
      "prefix": "app",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "allowedCommonJsDependencies": [
              "firebase",
              "@firebase/app",
              "firebase/app",
              "notiflix",
              "lodash",
              "hammerjs",
              "@firebase/functions",
              "@firebase/auth",
              "@firebase/util",
              "@firebase/component",
              "@firebase/performance"
            ],
            "outputPath": "dist/shop-version-2",
            "index": "projects/shop-version-2/src/index.html",
            "main": "projects/shop-version-2/src/main.ts",
            "polyfills": "projects/shop-version-2/src/polyfills.ts",
            "tsConfig": "projects/shop-version-2/tsconfig.app.json",
            "aot": true,
            "assets": [
              "projects/shop-version-2/src/favicon.ico",
              "projects/shop-version-2/src/assets",
              "projects/shop-version-2/src/firebase-messaging-sw.js",
              "projects/shop-version-2/src/copy-file.php",
              "projects/shop-version-2/src/manifest.webmanifest"
            ],
            "styles": [

              "projects/shop-version-2/src/styles.scss",
              "./node_modules/animate.css/animate.min.css",
              "./node_modules/cookieconsent/build/cookieconsent.min.css"
            ],
            "scripts": [
              "./node_modules/cookieconsent/build/cookieconsent.min.js"
            ]
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "projects/shop-version-2/src/environments/environment.ts",
                  "with": "projects/shop-version-2/src/environments/environment.prod.ts"
                }
              ],
              "index": "projects/shop-version-2/src/index.php",
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "6kb",
                  "maximumError": "10kb"
                }
              ],
              "serviceWorker": true,
              "ngswConfigPath": "projects/shop-version-2/ngsw-config.json"
            }
          }
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "shop-version-2:build"
          },
          "configurations": {
            "production": {
              "browserTarget": "shop-version-2:build:production"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "shop-version-2:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "projects/shop-version-2/src/test.ts",
            "polyfills": "projects/shop-version-2/src/polyfills.ts",
            "tsConfig": "projects/shop-version-2/tsconfig.spec.json",
            "karmaConfig": "projects/shop-version-2/karma.conf.js",
            "assets": [
              "projects/shop-version-2/src/favicon.ico",
              "projects/shop-version-2/src/assets",
              "projects/shop-version-2/src/firebase-messaging-sw.js",
              "projects/shop-version-2/src/copy-file.php",
              "projects/shop-version-2/src/manifest.webmanifest",
              "projects/shop-version-2/src/manifest.webmanifest"
            ],
            "styles": [
              "projects/shop-version-2/src/styles.scss"
            ],
            "scripts": []
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "projects/shop-version-2/tsconfig.app.json",
              "projects/shop-version-2/tsconfig.spec.json",
              "projects/shop-version-2/e2e/tsconfig.json"
            ],
            "exclude": [
              "**/node_modules/**"
            ]
          }
        },
        "e2e": {
          "builder": "@angular-devkit/build-angular:protractor",
          "options": {
            "protractorConfig": "projects/shop-version-2/e2e/protractor.conf.js",
            "devServerTarget": "shop-version-2:serve"
          },
          "configurations": {
            "production": {
              "devServerTarget": "shop-version-2:serve:production"
            }
          }
        }
      }
    }
  },
  "defaultProject": "cart",
  "cli": {
    "analytics": false
  }
}
